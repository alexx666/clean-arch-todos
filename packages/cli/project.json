{
	"name": "cli",
	"projectType": "application",
	"sourceRoot": "packages/cli/src",
	"prefix": "packages",
	"targets": {
		"build": {
			"executor": "@nrwl/node:webpack",
			"options": {
				"main": "packages/cli/src/todos.ts",
				"tsConfig": "packages/cli/tsconfig.app.json",
				"outputPath": "dist/packages/cli",
				"generatePackageJson": true,
				"sourceMap": true,
				"progress": true,
				"externalDependencies": "none"
			}
		},
		"start": {
			"executor": "nx:run-commands",
			"options": {
				"command": "node dist/packages/cli/main"
			}
		},
		"debug": {
			"executor": "nx:run-commands",
			"options": {
				"command": "node --inspect dist/packages/cli/main"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": [
					"packages/cli/**/*.ts"
				]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": [
				"{workspaceRoot}/coverage/packages/cli"
			],
			"options": {
				"jestConfig": "packages/cli/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": [
		"scope:client",
		"type:app"
	],
	"implicitDependencies": [
		"core"
	]
}