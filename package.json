{
	"name": "clean-arch",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"libs/*",
		"apps/*"
	],
	"scripts": {
		"lint": "npx nx lint web",
		"serve": "npx nx serve",
		"test": "jest --passWithNoTests -c jest.config.ts",
		"test:watch": "npm t -- --watch",
		"clean": "tsc -b --clean tsconfig.build.json",
		"prebuild": "npm run clean",
		"build": "tsc -b tsconfig.build.json",
		"build:watch": "npm run build -- -w",
		"build:docs": "typedoc --options .typedoc/config.json",
		"registry": "verdaccio -c .verdaccio/config.yml",
		"postinstall": "ngcc --properties es2020 browser module main",
		"prepare": "husky install",
		"commit": "cz"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~14.0.0",
		"@angular-eslint/eslint-plugin": "~14.0.0",
		"@angular-eslint/eslint-plugin-template": "~14.0.0",
		"@angular-eslint/template-parser": "~14.0.0",
		"@angular/cli": "~14.0.0",
		"@angular/compiler-cli": "~14.0.0",
		"@angular/language-service": "~14.0.0",
		"@angular/material": "github:angular/material2-builds",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@nrwl/angular": "^14.4.2",
		"@nrwl/cypress": "14.4.2",
		"@nrwl/eslint-plugin-nx": "14.4.2",
		"@nrwl/jest": "14.4.2",
		"@nrwl/linter": "14.4.2",
		"@types/jest": "^26.0.22",
		"@types/node": "^16.11.45",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"commitizen": "^4.2.5",
		"cypress": "^9.1.0",
		"eslint": "~8.15.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-jsdoc": "^39.3.4",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"husky": "^8.0.1",
		"jest": "^26.6.3",
		"jest-preset-angular": "~11.1.2",
		"nx": "14.4.2",
		"ts-jest": "^26.5.4",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.6",
		"typescript": "^4.7.4",
		"verdaccio": "^5.13.1"
	},
	"dependencies": {
		"@alexx666/todos": "*",
		"@angular/animations": "~14.0.0",
		"@angular/cdk": "github:angular/cdk-builds",
		"@angular/common": "~14.0.0",
		"@angular/compiler": "~14.0.0",
		"@angular/core": "~14.0.0",
		"@angular/forms": "~14.0.0",
		"@angular/platform-browser": "~14.0.0",
		"@angular/platform-browser-dynamic": "~14.0.0",
		"@angular/router": "~14.0.0",
		"@ngrx/effects": "^14.0.1",
		"@ngrx/store": "^14.0.1",
		"@ngrx/store-devtools": "^14.0.1",
		"rxjs": "^7.5.5",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.4"
	}
}
