{
	"name": "clean-arch",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"lint": "nx run-many --target=lint --all=true",
		"build": "nx run-many --target=build --all=true",
		"test": "nx run-many --target=test --all=true",
		"preserve": "docker compose up -d",
		"serve": "nx run-many --target=serve --all=true",
		"lint:affected": "nx affected --target=lint",
		"build:affected": "nx affected --target=build",
		"test:affected": "nx affected --target=test",
		"build:docs": "typedoc --options .typedoc/config.json",
		"publish": "nx run-many --target=publish --all=true",
		"postinstall": "ngcc --properties es2020 browser module main",
		"prepare": "husky install",
		"commit": "cz"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "15.0.3",
		"@angular-eslint/eslint-plugin": "15.0.0",
		"@angular-eslint/eslint-plugin-template": "15.0.0",
		"@angular-eslint/template-parser": "15.0.0",
		"@angular/cli": "~15.0.0",
		"@angular/compiler-cli": "15.0.3",
		"@angular/language-service": "15.0.3",
		"@angular/material": "15.0.2",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@nrwl/angular": "15.3.0",
		"@nrwl/cypress": "15.3.0",
		"@nrwl/esbuild": "15.3.0",
		"@nrwl/eslint-plugin-nx": "15.3.0",
		"@nrwl/jest": "15.3.0",
		"@nrwl/linter": "15.3.0",
		"@nrwl/node": "15.3.0",
		"@types/aws-lambda": "^8.10.109",
		"@types/jest": "28.1.1",
		"@types/node": "18.7.1",
		"@typescript-eslint/eslint-plugin": "5.46.0",
		"@typescript-eslint/parser": "5.46.0",
		"aws-sdk": "^2.1267.0",
		"commitizen": "^4.2.5",
		"cypress": "^9.1.0",
		"esbuild": "^0.15.17",
		"eslint": "~8.15.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-jsdoc": "^39.3.4",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"husky": "^8.0.1",
		"jest": "28.1.1",
		"jest-environment-jsdom": "28.1.1",
		"jest-preset-angular": "12.2.3",
		"nx": "15.3.0",
		"ts-jest": "28.0.5",
		"ts-node": "10.9.1",
		"typedoc": "^0.23.6",
		"typescript": "4.8.4",
		"verdaccio": "^5.13.1"
	},
	"dependencies": {
		"@angular/animations": "15.0.3",
		"@angular/cdk": "15.0.2",
		"@angular/common": "15.0.3",
		"@angular/compiler": "15.0.3",
		"@angular/core": "15.0.3",
		"@angular/forms": "15.0.3",
		"@angular/platform-browser": "15.0.3",
		"@angular/platform-browser-dynamic": "15.0.3",
		"@angular/router": "15.0.3",
		"@ngrx/effects": "^14.0.1",
		"@ngrx/store": "^14.0.1",
		"@ngrx/store-devtools": "^14.0.1",
		"commander": "^9.4.1",
